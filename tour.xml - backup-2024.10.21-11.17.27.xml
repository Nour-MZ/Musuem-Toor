	<action name="actioner" type="Javascript" autorun="onload"  ><![CDATA[
		console.log(krpano.actions);
		function transformSceneString(sceneString) {
			
			let modifiedString = sceneString.replace(/^scene_/, '');
			
			
			modifiedString = modifiedString.split('_').map((part, index) => {
				// Capitalize the first letter of each part, except for the first part
				if (index === 0) {
					return part; // Keep the first part as is
				}
				return part.charAt(0).toUpperCase() + part.slice(1).toLowerCase();
			}).join('_');

			// Append '.tiles' at the end
			return modifiedString + '.tiles';
		}

		console.log(krpano);
		for(let i=krpano.hotspot.count-1;i>0;i--) {
			const hotspotitem=krpano.hotspot.getItem(i);
			
			

				if(hotspotitem.name.startsWith("spot")){
					var linkedscene = transformSceneString(hotspotitem.linkedscene);

					console.log("linked",linkedscene)

					var name = "image" + hotspotitem.name;
					const newimageitem  = krpano.addhotspot(name );
					newimageitem.loadstyle("skin_hotspotstyles");
					newimageitem.setvars({
						keep:false, 
						
						
						
						distorted: false,
						depth: 4000,
						atv: hotspotitem.atv - 4,
						ath: hotspotitem.ath,
						renderer: "css3d",
						onloaded: "set_css_style('border-radius', '50%', 'background-color', 'pink');" ,
						alpha: 0,
						url : `panos/${linkedscene}/thumb.jpg`,
					});
					console.log(newimageitem);
					hotspotitem.setvars({
						 onhover: `addimage(${name})`,
						 onout: `removeimage(${name})`,
						
						 

					});
				}
			}
		console.log(krpano.hotspot.getArray());
		

	
		]]></action>

	<action name="setuptour">
		set(idletime,3);
